{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Prisma: Generate",
      "type": "shell",
      "command": "npx prisma generate",
      "options": { "cwd": "${workspaceFolder}/frontend" }
    },
    {
      "label": "Prisma: Migrate (dev)",
      "type": "shell",
      "command": "npx prisma migrate dev --name init",
      "options": { "cwd": "${workspaceFolder}/frontend" }
    },
    {
      "label": "Prisma: Seed",
      "type": "shell",
      "command": "npm run prisma:seed",
      "options": { "cwd": "${workspaceFolder}/frontend" }
    },
    {
      "label": "App: Dev",
      "type": "npm",
      "script": "dev",
      "options": { "cwd": "${workspaceFolder}/frontend" },
      "group": "build",
      "problemMatcher": []
    }
  ]
}