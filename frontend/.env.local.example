# Copy to .env.local and set values
NEXTAUTH_SECRET=replace-with-a-long-random-string
# Postgres connection string, e.g. for local dockerized PostgreSQL
# DATABASE_URL=postgresql://postgres:postgres@localhost:5432/poseidon_lms?schema=public
DATABASE_URL=

